{
  "name": "calculator-electron",
  "version": "0.0.0",
  "description": "Simple calculator.",
  "main": "dist/main.js",
  "author": "tinmn9088",
  "scripts": {
    "clean": "node ./node_modules/rimraf/bin.js dist/",
    "compile:scss": "sass src:dist",
    "compile:html": "copyfiles -u 1 src/**/*.html dist/",
    "compile": "npm run clean && tsc && npm run compile:scss && npm run compile:html",
    "start": "npm run compile && electron ."
  },
  "devDependencies": {
    "copyfiles": "^2.4.1",
    "electron": "^21.2.0",
    "rimraf": "^3.0.2",
    "sass": "^1.55.0",
    "typescript": "^4.8.4"
  }
}
